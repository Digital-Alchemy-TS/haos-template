    "eslint": "9.26.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "figlet-cli": "^0.3.0",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "lolcatjs": "^2.4.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "tslib": "^2.8.1",
    "tsx": "^4.19.4",
    "type-fest": "^4.41.0",
    "typescript": "^5.8.3"
