    "@digital-alchemy/synapse": "^24.11.4",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "ini": "^5.0.0",
    "node-cron": "^3.0.3",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@digital-alchemy/type-writer": "^24.11.3",
    "@eslint/compat": "^1.2.3",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "@types/node": "^22.10.1",
    "@types/node-cron": "^3.0.11",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "8.16.0",
    "@typescript-eslint/parser": "8.18.2",
    "eslint": "9.16.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "figlet-cli": "^0.2.0",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "lolcatjs": "^2.4.3",
    "nodemon": "^3.1.7",
    "prettier": "^3.4.1",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "type-fest": "^4.29.0",
    "typescript": "^5.7.2"
