{
  "name": "home_automation",
  "version": "0.1.0",
  "description": "A quick start demo workspace to building code with @digital-alchemy. Tuned for supervised Home Assistant installs",
  "scripts": {
    "build:deploy": "./scripts/deploy.sh",
    "build": "tsc",
    "develop:watch": "tsx watch src/main.ts",
    "develop": "tsx src/main.ts",
    "lint:fix": "eslint --fix src/",
    "lint": "eslint src/",
    "setup:addon": "./scripts/addon.sh",
    "setup:environment": "./scripts/environment.sh",
    "type-writer": "type-writer",
    "upgrade": "./scripts/update_deps.sh"
  },
  "license": "MIT",
  "dependencies": {
    "@digital-alchemy/core": "^0.1.14"
  },
  "devDependencies": {
    "@cspell/eslint-plugin": "^8.5.0",
    "@types/async": "^3.2.24",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsonc": "^2.13.0",
    "eslint-plugin-no-unsanitized": "^4.0.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-security": "^2.1.1",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-sonarjs": "^0.24.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unicorn": "^51.0.1",
    "figlet-cli": "^0.1.1",
    "lolcatjs": "^2.4.3",
    "npm-check-updates": "^16.14.15",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "type-fest": "^4.11.0",
    "typescript": "^5.3.3"
  }
