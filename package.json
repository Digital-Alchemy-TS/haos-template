    "@digital-alchemy/synapse": "^25.3.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "ini": "^5.0.0",
    "node-cron": "^4.0.2",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@digital-alchemy/type-writer": "^25.3.2",
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.26.0",
    "@types/node": "^22.15.17",
    "@types/node-cron": "^3.0.11",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "8.32.0",
